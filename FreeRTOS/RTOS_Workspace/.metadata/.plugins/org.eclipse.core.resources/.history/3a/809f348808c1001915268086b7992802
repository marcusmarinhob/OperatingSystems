#include "stm32f4xx.h"
			

int main(void)
{

	for(;;)
	{
		BaseType_t xTaskCreate( TaskFunction_t vTaskCode,
								const char * const pcName,
								unsigned short usStackDepth,
								void *pvParameters,
								UBaseType_t uxPriority,
								TaskHandle_t *pxCreatedTask
							  );
	}
}
